{
  "name": "@amakovskyi/api-auditor",
  "version": "0.2.1",
  "type": "module",
  "keywords": [
    "integration",
    "test",
    "matcher",
    "testing",
    "validation"
  ],
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amakovskyi/api-auditor.git"
  },
  "author": "amakovskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amakovskyi/api-auditor/issues"
  },
  "homepage": "https://github.com/amakovskyi/api-auditor#readme",
  "dependencies": {
    "ts-node": "^10.7.0",
    "jest": "^27.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "ts-jest": "^27.1.4",
    "@types/uuid": "^8.3.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 10000
  }
}
